# =============================================================================
# Dify Environment Configuration Example
# =============================================================================
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control
# =============================================================================

# Application Settings
MODE=production
LOG_LEVEL=INFO
SECRET_KEY=your-secret-key-here
DEBUG=false

# Database Configuration
DB_USERNAME=your-db-username
DB_PASSWORD=your-db-password
DB_HOST=your-db-host
DB_PORT=5432
DB_DATABASE=dify

# Redis Configuration
REDIS_HOST=redis
REDIS_PASSWORD=your-redis-password
REDIS_PORT=6379
REDIS_DB=0

# Web Configuration
CONSOLE_WEB_URL=https://your-domain.com
CONSOLE_API_URL=https://your-domain.com
APP_WEB_URL=https://your-domain.com
APP_API_URL=https://your-domain.com
WEB_API_CORS_ALLOW_ORIGINS=https://your-domain.com
SERVICE_API_URL=https://your-domain.com

# File Storage (optional)
STORAGE_TYPE=local
STORAGE_LOCAL_PATH=storage

# Email Configuration (optional)
MAIL_TYPE=smtp
MAIL_DEFAULT_SEND_FROM=admin@your-domain.com
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-email-app-password
SMTP_USE_TLS=true
SMTP_USE_SSL=false

# Email Features
ENABLE_EMAIL_CODE_LOGIN=true
ENABLE_EMAIL_PASSWORD_LOGIN=true
MAIL_USE_TLS=true
MAIL_USE_SSL=false
SYSTEM_EMAIL_TEMPLATE_ENABLED=true

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_CLOUD_REGION=your-gcp-region
GOOGLE_CLOUD_ZONE=your-gcp-zone

# Database Connection (Cloud SQL)
CLOUD_SQL_CONNECTION_NAME=your-project:your-region:your-db-instance
